{
  "name": "@jlguenego/asn.1",
  "version": "0.0.4",
  "description": "ASN.1 node tools",
  "private": false,
  "main": "build/src/index.js",
  "bin": {
    "asn1-parse": "./bin/asn1-parse.js"
  },
  "scripts": {
    "test": "nyc mocha",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint",
    "release": "standard-version",
    "start": "node bin/asn1-parse.js -f hex test/data/foo-question.der -d test/data/foo-protocol.asn1 -t FooQuestion",
    "kerberos": "node bin/asn1-parse.js -f base64 test/data/kerberos.base64.der -d test/data/kerberos.asn1 -t GSS-API"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.12",
    "gts": "^3.0.2",
    "mocha": "^8.2.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "chevrotain": "^7.0.3",
    "commander": "^6.2.0",
    "debug": "^4.3.1",
    "nyc": "^15.1.0"
  }
}
